{
	"manifest_version":2,
	"name": "Browser Buddy",
	"description": "extension to incetevize productivity",
	"version" : "1.0",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	  	"permissions": [
		"storage",
		"tabs",
		"background",
		"alarms"
]
,"background":{
"page": "background.html"
},
"background.persistent": {"persistent":true}
}